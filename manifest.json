{
    "name": "BeeSync",
    "description": "Sync YouTube videos with your stream viewers, AdBlock recommended.",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "beesync.js"
    },
    "options_page": "options.html",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/white_16.png",
            "32": "/images/white_32.png",
            "48": "/images/white_48.png",
            "128": "/images/white_128.png"
        }
    },
    "icons": {
        "16": "/images/white_16.png",
        "32": "/images/white_32.png",
        "48": "/images/white_48.png",
        "128": "/images/white_128.png"
    },
    "oauth2": {
        "client_id": "1043280135218-jbm7lqmbulot7einkuqe6mevqv0p42n2.apps.googleusercontent.com",
        "scopes": ["youtube.googleapis.com"]
    }
}
